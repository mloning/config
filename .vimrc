color desert

syntax enable
set showmode
set backspace=indent,eol,start

set tabstop=4
set shiftwidth=4
set noexpandtab
set autoindent
"set breakindent
set smartindent
set cindent

set ignorecase
set smartcase
set nowrapscan
set incsearch

set wildmenu
set wildmode=full

set number 

set cursorline
"set cursorcolumn

set spelllang=en

filetype indent plugin on
